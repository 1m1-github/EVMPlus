var contract = eth.contract(abi);
var gas = eth.estimateGas({ data: bytecode });
var tx = { from: eth.accounts[0], data: bytecode, gas: gas };
var deployed_contract = contract.new(tx);

var instance = contract.at('0xe8401561c18a51d4bd871750317b5f55f75996af');
instance.addone.sendTransaction(42, { from: eth.accounts[0], gas: 1000000 });

